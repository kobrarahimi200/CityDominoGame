package logic;

/**
 *
 * @author kobra
 */
public class Choose {
    
    private final Tiles tile;
    private  Players currPlayer;

    public Choose(Tiles tile, Players player) {
        this.tile = tile;
        this.currPlayer = player;
    }
    
     Players getPlayer() {
        return this.currPlayer;
    } 
    
     Tiles getTile() {
        return this.tile;
    }

}
