package logic;

/**
 *
 * @author kobra
 */
public class Bot extends Players{
    
    /**
     * default constructor
     * @param name 
     * @param field 
     */
     public Bot(String name, Field field) {
        super( "Bot" +name, field);
    }
     
    public Bot(String name, int x, int y) {
        super( "Bot" +name, x, y);
    }
    /**
     * assigns the name and the tile from the parent class.
     *
     * @param name
     * @param tile
     */
    public Bot(String name, Tiles tile) {
        super("R" +name, tile);
    }
}
